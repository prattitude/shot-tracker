{
  "name": "Billiards Analytics",
  "short_name": "Pool Analytics",
  "description": "Real-time billiards game analytics and timing tracker",
  "start_url": "/",
  "display": "standalone",
  "background_color": "#1a472a",
  "theme_color": "#1a472a",
  "orientation": "portrait-primary",
  "categories": ["sports", "games", "utilities"],
  "lang": "en",
  "icons": [
    {
      "src": "data:image/svg+xml;base64,",
      "sizes": "192x192",
      "type": "image/svg+xml",
      "purpose": "any"
    },
    {
      "src": "data:image/svg+xml;base64,",
      "sizes": "512x512",
      "type": "image/svg+xml",
      "purpose": "any"
    }
  ],
  "shortcuts": [
    {
      "name": "New Game",
      "short_name": "New",
      "description": "Start a new billiards game",
      "url": "/?action=new",
      "icons": [
        {
          "src": "data:image/svg+xml;base64,",
          "sizes": "96x96",
          "type": "image/svg+xml"
        }
      ]
    }
  ],
  "scope": "/",
  "prefer_related_applications": false,
  "capture_links": "new-client",
  "launch_handler": {
    "client_mode": "focus-existing"
  },
  "edge_side_panel": {
    "preferred_width": 400
  },
  "protocol_handlers": [{
    "protocol": "web+billiards",
    "url": "/?action=%s"
  }]
}